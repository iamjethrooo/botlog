{
  "extends": "@sapphire/ts-config",
  "compilerOptions": {
    "target": "ES6",
    "rootDir": "src",
    "incremental": true,
    "outDir": "dist",
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "resolveJsonModule": true,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "module": "es2020"
  },
  "include": ["src"]
}
